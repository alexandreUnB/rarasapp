<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"

    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="10dp"
    android:paddingBottom="25dp"
    android:paddingTop="10dp"
    android:paddingLeft="15dp"
    android:paddingRight="15dp"
    android:background="@drawable/main_bg">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">


        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/supportersTitle"
            android:background="#ffffff"
            android:src="@mipmap/logo_unb"
            android:layout_margin="10dp" />

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#ffffff"
            android:layout_below="@id/supportersTitle"
            android:src="@mipmap/logo_rnp"
            android:layout_margin="10dp"

            />

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#ffffff"
            android:layout_below="@id/supportersTitle"
            android:src="@mipmap/logo_telesaude"
            android:layout_margin="10dp" />

        <ImageView

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/supportersTitle"
            android:src="@mipmap/logo_sgtes"
            android:layout_margin="10dp"
            android:background="#ffffff"

            />

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/supportersTitle"
            android:background="#ffffff"
            android:src="@mipmap/logo_orphanet"
            android:layout_margin="10dp" />

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#ffffff"
            android:layout_below="@id/supportersTitle"
            android:src="@mipmap/logo_nesp"
            android:layout_margin="10dp" />


    </LinearLayout>


</RelativeLayout>
